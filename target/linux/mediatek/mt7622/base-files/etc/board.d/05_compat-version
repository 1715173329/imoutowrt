
. /lib/functions.sh
. /lib/functions/uci-defaults.sh

board_config_update

case "$(board_name)" in
	bananapi,bpi-r64)
		ucidef_set_compat_version "1.2"
		;;
	linksys,e8450-ubi)
		ucidef_set_compat_version "2.0"
		;;
	tplink,tl-xdr3230-v1|\
	tplink,tl-xdr3250-v1)
		ucidef_set_compat_version "1.1"
		;;
esac

board_config_flush

exit 0
